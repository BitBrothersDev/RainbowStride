<div class="container mt-4">
  <div class="row">
    <div class="col">
      <h1>Editing Regimen Blueprint</h1>
    </div>
  </div>

  <div class="row">
    <div class="col">
      <%= render "form", regimen_blueprint: @regimen_blueprint %>
    </div>
  </div>

  <div class="row mt-3">
    <div class="col">
      <%= link_to "Show this regimen blueprint", @regimen_blueprint, class: "btn btn-outline-primary mr-2" %>
      <%= link_to "Back to regimen blueprints", regimen_blueprints_path, class: "btn btn-outline-secondary" %>
    </div>
  </div>
</div>